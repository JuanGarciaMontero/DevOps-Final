# Jenkinsfile

@Library('github.com/JuanGarciaMontero/DevOps-Final@QA') _

pipeline {
    agent any
    stages {
        stage ('Stage crea contenedor con Python') {
                   agent {
                       docker { image 'python:slim-bullseye' }
                   }
                   steps {
                       echo 'Stage con ejecuci√≥n normal, python'
                               LibreriaContenedor()
                   }
        }
        }
post { always { echo "Fin del pipeline" } }              
} 